														<xs:sequence>
															<xs:element name="str_b_vw" type="maxLength2StringType"/>
															<xs:element name="str_b_bhfnr" type="maxLength6StringType"/>
															<xs:element name="str_b_bhfname" type="maxLength40StringType" minOccurs="0"/>
